{
  "name": "dummy-pics",
  "version": "0.0.1",
  "description": "Dummy images generator",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run remove && tsc && npm run copy",
    "copy": "cp -r src/views dist/views",
    "remove": "rm -rf dist",
    "lint": "eslint --ext .ts src/",
    "start": "npm run build && node dist/index.js",
    "tsc:init": "tsc --init",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vcgtz/dummy-pics.git"
  },
  "keywords": [
    "dummy",
    "generator",
    "images",
    "placeholders"
  ],
  "author": "Vicente Guti√©rrez (gtz.rvera@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vcgtz/dummy-pics/issues"
  },
  "homepage": "https://github.com/vcgtz/dummy-pics#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "hbs": "^4.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hbs": "^4.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^4.9.5"
  }
}
